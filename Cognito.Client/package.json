{
    "name": "memento",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:ssr": "ng run memento:server:dev",
        "test": "ng test",
        "lint": "eslint \"**/*.{js,ts}\" --fix",
        "e2e": "ng e2e",
        "build-serve": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve",
        "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,ts}": [
            "npx prettier --write",
            "eslint"
        ]
    },
    "private": true,
    "dependencies": {
